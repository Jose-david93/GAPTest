
@{
    ViewData["Title"] = "Index";
}
<link href="~/css/Dates.css" rel="stylesheet" />
<div class="contenedor">
    <form action="" class="form">
        <div class="form-header">
            <div class="card-footer">
                <p class="alert-warning text-hide" id="mensaje">creada</p>
            </div>
            <h1 class="form-tittle">Citas</h1>
            <table id="dates" class="table table-striped table-bordered" style="width:100%">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Dni</th>
                        <th>Nombre Paciente</th>
                        <th>Apellido</th>
                        <th>Servicio</th>
                        <th>Descripcion</th>
                        <th>Estado</th>
                        <th>Fecha/Hora</th>
                        <th>Opciones</th>
                    </tr>
                </thead>
            </table>

        </div>
    </form>
</div>

<script src="~/js/Dates.js"></script>